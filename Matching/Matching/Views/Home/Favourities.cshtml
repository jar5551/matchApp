@model Matching.Models.Favourities
@{
    ViewBag.Title = "Favourities";
}

<h2>Favourities</h2>

@using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.ValidationSummary("", new { @class = "text-danger" })

    <div class="form-group">

        <div class="col-md-10">
            @Html.TextBoxFor(m => m.favourities, new { @class = "form-control" })
        </div>
    </div>
}